{
  "name": "clay-cljs",
  "version": "0.1.0",
  "description": "Clay UI ClojureScript with Missionary flows and real-time mouse tracking",
  "private": true,
  "scripts": {
    "watch": "shadow-cljs watch mouse-tracker",
    "watch:mouse": "shadow-cljs watch mouse-tracker",
    "watch:v2": "shadow-cljs watch mouse-tracker-v2",
    "compile": "shadow-cljs compile mouse-tracker",
    "compile:mouse": "shadow-cljs compile mouse-tracker",
    "compile:v2": "shadow-cljs compile mouse-tracker-v2",
    "release": "shadow-cljs release mouse-tracker",
    "release:mouse": "shadow-cljs release mouse-tracker",
    "release:v2": "shadow-cljs release mouse-tracker-v2",
    "watch:v3": "shadow-cljs watch mouse-tracker-v3",
    "compile:v3": "shadow-cljs compile mouse-tracker-v3",
    "release:v3": "shadow-cljs release mouse-tracker-v3",
    "server": "shadow-cljs server",
    "clean": "rm -rf resources/public/js resources/public/v2/js resources/public/v3/js .shadow-cljs"
  },
  "devDependencies": {
    "shadow-cljs": "^2.27.5"
  }
}

